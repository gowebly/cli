{
  "name": "{{ .PackageName }}",
  "version": "0.0.0",
  "description": "Frontend part of the Gowebly project.",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "source": [
    "./web/src/js/scripts.js",
    "./web/src/scss/styles.scss"
  ],
  "targets": {
    "default": {
      "distDir": "./web/static"
    }
  },
  "scripts": {
    "watch": "parcel watch",
    "dev": "parcel build --no-optimize",
    "build": "parcel build"
  },
  "dependencies": {
    {{ if or .CSSFramework "daisyui" "flowbite" "tailwindcss" }}
    "@tailwindcss/typography": "latest",
    "tailwindcss": "latest",
    {{ end }}
    {{ if eq .CSSFramework "bootstrap" }}
    "bootstrap": "latest",
    {{ else if eq .CSSFramework "bulma" }}
    "bulma": "latest",
    {{ else if eq .CSSFramework "daisyui" }}
    "daisyui": "latest",
    {{ else if eq .CSSFramework "flowbite" }}
    "flowbite": "latest",
    {{ else if eq .CSSFramework "unocss" }}
    "unocss": "latest",
    {{ end }}
    "htmx.org": "latest",
    "hyperscript.org": "latest"
  },
  "devDependencies": {
    {{ if eq .CSSFramework "bootstrap" }}
    "@popperjs/core": "latest",
    {{ else if eq .CSSFramework "unocss" }}
    "@unocss/cli": "latest",
    "@unocss/postcss": "latest",
    {{ end }}
    "postcss": "latest",
    "parcel": "latest"
  }
}
